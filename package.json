{
  "name": "ipo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "b:lint": "bunx @biomejs/biome lint --apply .",
    "b:check": "bunx @biomejs/biome check --apply .",
    "b:unsafe": "bunx @biomejs/biome check --apply-unsafe  .",
    "b:format": "bunx @biomejs/biome format --write .",
    "push": "bun run b:format && bun run lint && bun run b:unsafe && git add . && git commit -m \"update\" && git push"
  },
  "dependencies": {
    "react": "^18",
    "react-dom": "^18",
    "next": "14.2.3"
  },
  "devDependencies": {
    "@biomejs/biome": "1.7.3",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
